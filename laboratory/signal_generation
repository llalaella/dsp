NAL"],"metadata":{"id":"uvlle3AuHjAR"}},{"cell_type":"code","source":["\n","import matplotlib.pyplot as plt\n","import math\n","\n","duration = 5\n","sampling_rate = 100\n","decay_rate = 0.5\n","\n","t = [i / sampling_rate for i in range(duration * sampling_rate)]\n","\n","\n","exponential_signal = [math.exp(-decay_rate * time) for time in t]\n","\n","plt.figure(figsize=(10, 4))\n","plt.plot(t, exponential_signal)\n","plt.title(\"Exponential Signal\")\n","plt.xlabel(\"Time (s)\")\n","plt.ylabel(\"Amplitude\")\n","plt.grid()\n","plt.xlim(0, duration)\n","plt.ylim(0, 1.1)\n","plt.show()"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":172},"id":"BxG80rxy1hN9","executionInfo":{"status":"ok","timestamp":1728105761580,"user_tz":-480,"elapsed":623,"user":{"displayName":"Mariella Faith Napuran","userId":"14097573403958381992"}},"outputId":"35e501f5-4f61-4cc6-c099-a38bb9a13093"},"execution_count":null,"outputs":[{"output_type":"display_data","data":{"text/plain":["<Figure size 1000x400 with 1 Axes>"],"image/png":"\n"},"metadata":{}}]},{"cell_type":"markdown","source":["SQUARE WAVE SIGNAL"],"metadata":{"id":"c_6AOTFcHY5I"}},{"cell_type":"code","source":["\n","import matplotlib.pyplot as plt\n","\n","duration = 5\n","sampling_rate = 100\n","frequency = 1\n","\n","t = [i / sampling_rate for i in range(duration * sampling_rate)]\n","\n","square_wave_signal = [1 if (i // (sampling_rate // (frequency * 2))) % 2 == 0 else -1 for i in range(len(t))]\n","\n","plt.figure(figsize=(10, 4))\n","plt.plot(t, square_wave_signal, drawstyle='steps-post')\n","plt.title(\"Square Wave Signal\")\n","plt.xlabel(\"Time (s)\")\n","plt.ylabel(\"Amplitude\")\n","plt.grid()\n","plt.xlim(0, duration)\n","plt.ylim(-1.5, 1.5)\n","plt.show()"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":170},"id":"vC8uRygA1q_R","executionInfo":{"status":"ok","timestamp":1728100906448,"user_tz":-480,"elapsed":500,"user":{"displayName":"Mariella Faith Napuran","userId":"14097573403